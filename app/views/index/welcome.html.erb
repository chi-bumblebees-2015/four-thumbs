<h1>Four Thumbs</h1>
<% @movies.each do |movie| %>
  <li><%= link_to movie.name, movie, id: "movie-#{movie.id}" %></li>
<% end %>
<a id="new_user" href="/users/new">New User</a>
<a id="login" href="/login">Log In</a>
<a id="logout" href="/logout">Log out</a>
<br>
<% if current_user %>
  <%= current_user.name %>
<% end %>
